#include <stdio.h>
	
int main() {
    const int size = 100;
    int numbers[size];

    printf("Введіть 100 цілих чисел:\n");
    for (int i = 0; i < size; ++i) {
        printf("Число %d: ", i + 1);
        scanf("%d", &numbers[i]);
    }

    printf("Від'ємні числа:\n");
    for (int i = 0; i < size; ++i) {
        if (numbers[i] < 0) {
            printf("%d ", numbers[i]);
        }
    }

    printf("\nІнші числа:\n");
    for (int i = 0; i < size; ++i) {
        if (numbers[i] >= 0) {
            printf("%d ", numbers[i]);
        }
    }

    return 0;
}
