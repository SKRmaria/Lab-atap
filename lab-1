#include <stdio.h>
//бібліотека математичних виразів для С
#include <math.h>


int main() {
//елементи, що приймають участь у коді
    double z1, z2, a, p;
//значення P
    p = 3.14;
// те, що потрібно зчитати з клавіатури
    scanf ("%lf", &a);
// Перетворенне у формат коду C завдання з лабораторної
    z1 = (sin(4*a)/(1+cos(4*a))*(cos(2*a)/(1+cos(2*a))));
// відображення на терміналі
    printf("%lg\n", z1);
// Перетворенне у формат коду C завдання з лабораторної
    z2 = ctan(((3/2)*p)-a);
// відображення на терміналі
    printf("%lg\n", z2);

    return 0;
}
